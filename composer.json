{
    "name": "dkarlovi/dockerfile",
    "description": "Dockerfile generation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dkarlovi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dkarlovi\\Dockerfile\\": "src/Dockerfile/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkarlovi\\Dockerfile\\Test\\": "tests/Dockerfile/Test/"
        }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^6.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
