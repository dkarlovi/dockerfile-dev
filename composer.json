{
    "name": "dkarlovi/dockerfile-dev",
    "description": "Dockerfile generation mono-repo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dkarlovi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dkarlovi\\Dockerfile\\Builder\\": "src/dockerfile-builder/Dockerfile/Builder/",
            "Dkarlovi\\Dockerfile\\": "src/dockerfile/Dockerfile/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkarlovi\\Dockerfile\\Builder\\Test\\": "src/dockerfile-builder/Dockerfile/Builder/Test/",
            "Dkarlovi\\Dockerfile\\Test\\": "src/dockerfile/Dockerfile/Test/"
        }
    },
    "bin": [
        "src/dockerfile-builder/bin/dockerfile-builder"
    ],
    "require": {
        "php": "^7.1",
        "ext-mbstring": "^7.1",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^6.2",
        "symfony/console": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
